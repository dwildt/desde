  "description": "Rastreador de hábitos - Desde quando você tem algum hábito?",
  "main": "scripts/app.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
