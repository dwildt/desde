  "bugs": {
    "url": "https://github.com/dwildt/desde/issues"
  },
  "homepage": "https://dwildt.github.io/desde/",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.48.0",
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
